alias esvm="ssh forge@159.203.189.34"   # Estated Production
alias usvm="ssh forge@159.203.196.243"  # USRR Production

# misc
alias editaliases="vi ~/.aliases"
alias editvimrc="vi ~/.vimrc"
alias zshconfig="vi ~/.zshrc"
alias editzshrc="vi ~/.zshrc"
alias ohmyzsh="vi ~/.oh-my-zsh"
